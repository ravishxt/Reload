{
    "manifest_version": 3,
    "name": "Reload",
    "version": "1.0",
    "description": "An extension to empty cache and hard reload the page.",
    "permissions": [
      "activeTab",
      "scripting",
      "cookies"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  